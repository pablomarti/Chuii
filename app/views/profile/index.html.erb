<div id="ideasContainer" class="leftFeed">
	<div>
		<%= link_to "Popular ideas", loadProfileIdeas_path(@selectedUser, 1), :remote => true %>
		<%= link_to "Recent ideas", loadProfileIdeas_path(@selectedUser, 2), :remote => true %>
	</div>

	<div id="feed">
		<%= render :partial => "idea", :collection => @ideas %>
	</div>
</div>

<div id="newIdea" class="rightBox">
	<%= render :partial => "newIdea" %>
</div>

<%= clearBox %>